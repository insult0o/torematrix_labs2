{
  "name": "High Contrast Light",
  "version": "1.0.0",
  "metadata": {
    "description": "High contrast light theme for maximum accessibility (WCAG AAA)",
    "author": "TORE Matrix Labs",
    "category": "accessibility",
    "accessibility_compliant": true,
    "high_contrast_available": true,
    "keywords": ["high-contrast", "accessibility", "wcag", "light"]
  },
  "colors": {
