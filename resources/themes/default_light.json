  "components": {
    "button": {
      "padding_x": "16px",
      "padding_y": "8px",
      "border_radius": "8px",
      "font_weight": "500",
      "transition": "all 150ms ease"
    },
    "input": {
      "padding_x": "12px",
      "padding_y": "8px",
      "border_radius": "8px",
      "font_size": "14px",
      "transition": "border-color 150ms ease"
    },
    "card": {
      "padding": "24px",
      "border_radius": "12px",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
    },
    "modal": {
      "backdrop_color": "rgba(0, 0, 0, 0.5)",
      "border_radius": "12px",
      "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1)"
    }
  },
  "accessibility": {
    "high_contrast": false,
    "focus_ring_color": "#3b82f6",
    "focus_ring_width": "2px",
    "focus_ring_offset": "2px",
    "min_contrast_ratio": 4.5,
    "touch_target_size": "44px"
