    "text_secondary": "#000000",
    "text_disabled": "#666666",
    "border": "#000000",
    "border_focus": "#0000ff",
    "selection_background": "#0000ff",
    "selection_text": "#ffffff",
    "success": "#008000",
    "warning": "#ff8c00",
    "error": "#ff0000",
    "info": "#0000ff",
    "button_primary_background": "#000000",
    "button_primary_text": "#ffffff",
    "button_primary_border": "#000000",
    "button_primary_hover": "#333333",
    "button_primary_pressed": "#555555",
    "button_secondary_background": "#ffffff",
    "button_secondary_text": "#000000",
    "button_secondary_border": "#000000",
    "button_secondary_hover": "#f5f5f5",
    "button_secondary_pressed": "#e0e0e0",
    "input_background": "#ffffff",
    "input_text": "#000000",
    "input_border": "#000000",
    "input_border_focus": "#0000ff",
    "input_placeholder": "#666666",
    "sidebar_background": "#ffffff",
    "sidebar_text": "#000000",
    "sidebar_border": "#000000",
    "header_background": "#ffffff",
    "header_text": "#000000",
    "header_border": "#000000",
    "footer_background": "#ffffff",
    "footer_text": "#000000",
    "footer_border": "#000000",
    "icon_primary": "#000000",
    "icon_secondary": "#333333",
    "icon_disabled": "#666666",
    "icon_accent": "#0000ff",
    "link": "#0000ff",
    "link_hover": "#000080",
    "link_visited": "#800080",
    "shadow_light": "rgba(0, 0, 0, 0.3)",
    "shadow_medium": "rgba(0, 0, 0, 0.5)",
    "shadow_heavy": "rgba(0, 0, 0, 0.7)"
  },
  "typography": {
    "font_family_primary": "Arial, sans-serif",
    "font_family_secondary": "Arial, sans-serif",
    "font_family_mono": "Courier New, monospace",
    "font_size_xs": "14px",
    "font_size_sm": "16px",
    "font_size_md": "18px",
    "font_size_lg": "20px",
    "font_size_xl": "22px",
    "font_size_2xl": "26px",
    "font_size_3xl": "32px",
    "font_weight_light": "400",
    "font_weight_normal": "400",
    "font_weight_medium": "700",
    "font_weight_semibold": "700",
    "font_weight_bold": "700",
    "line_height_tight": "1.3",
    "line_height_normal": "1.6",
    "line_height_relaxed": "1.8",
    "letter_spacing_tight": "0em",
    "letter_spacing_normal": "0em",
    "letter_spacing_wide": "0.1em"
  },
  "spacing": {
    "xs": "6px",
    "sm": "12px",
    "md": "20px",
    "lg": "28px",
    "xl": "36px",
    "2xl": "52px",
    "3xl": "68px"
  },
  "borders": {
    "radius_sm": "2px",
    "radius_md": "4px",
    "radius_lg": "6px",
    "radius_xl": "8px",
    "radius_full": "0px",
    "width_thin": "2px",
    "width_medium": "3px",
    "width_thick": "4px"
  },
  "shadows": {
    "sm": "0 2px 4px 0 rgba(0, 0, 0, 0.3)",
    "md": "0 4px 8px -1px rgba(0, 0, 0, 0.5), 0 2px 4px -1px rgba(0, 0, 0, 0.3)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.5), 0 4px 6px -2px rgba(0, 0, 0, 0.3)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.5), 0 10px 10px -5px rgba(0, 0, 0, 0.3)",
    "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.3)"
  },
  "animations": {
    "duration_fast": "0ms",
    "duration_normal": "0ms",
    "duration_slow": "0ms",
    "easing_linear": "linear",
    "easing_ease": "linear",
    "easing_ease_in": "linear",
    "easing_ease_out": "linear",
    "easing_ease_in_out": "linear"
  },
  "components": {
    "button": {
      "padding_x": "20px",
      "padding_y": "12px",
      "border_radius": "4px",
      "font_weight": "700",
      "transition": "none"
    },
    "input": {
      "padding_x": "16px",
      "padding_y": "12px",
      "border_radius": "4px",
      "font_size": "16px",
      "transition": "none"
    },
    "card": {
      "padding": "28px",
      "border_radius": "6px",
      "shadow": "0 4px 8px -1px rgba(0, 0, 0, 0.5)"
    },
    "modal": {
      "backdrop_color": "rgba(0, 0, 0, 0.8)",
      "border_radius": "6px",
      "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.5)"
    }
  },
  "accessibility": {
    "high_contrast": true,
    "focus_ring_color": "#0000ff",
    "focus_ring_width": "3px",
    "focus_ring_offset": "3px",
    "min_contrast_ratio": 7.0,
    "touch_target_size": "48px"
  }
}